	<set:Security>
		<set:Authentication>
			<set:method>user/password</set:method>
			<set:AuthenticationServer>http://www.lat-lon.de
			</set:AuthenticationServer>
		</set:Authentication>
	</set:Security>
	<set:Formats>
		<set:Format pattern="##.000" name="number"/>
	</set:Formats>
